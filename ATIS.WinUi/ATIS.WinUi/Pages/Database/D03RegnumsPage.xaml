        <DataTemplate x:Key="RegnumButtonItemTemplate" x:DataType="database:D03RegnumsViewModel">
            <StackPanel>
                <Button
                    Width="60"
                    Margin="2"
                    Command="{x:Bind AddRegnumCommand, Mode=OneWay}"
                    Content="Add"
                    CornerRadius="15" />
                <Button
                    Width="60"
                    Margin="2"
                    Command="{x:Bind SaveRegnumCommand, Mode=OneWay}"
                    Content="Save"
                    CornerRadius="15" />
                <Button
                    Width="60"
                    Margin="2"
                    Command="{x:Bind CopyRegnumCommand, Mode=OneWay}"
                    Content="Copy"
                    CornerRadius="15" />
                <Button
                    Width="60"
                    Margin="2"
                    Command="{x:Bind DeleteRegnumCommand, Mode=OneWay}"
                    Content="Delete"
                    CornerRadius="15" />
            </StackPanel>

        </DataTemplate>
        <DataTemplate x:Key="RegnumItemTemplate" x:DataType="models:Tbl03Regnum">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <Ellipse
                    x:Name="Ellipse"
                    Grid.RowSpan="2"
                    Width="32"
                    Height="32"
                    Margin="6"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    Fill="{ThemeResource SystemControlBackgroundBaseMediumBrush}" />
                <TextBlock
                    Grid.Row="0"
                    Grid.Column="1"
                    Margin="12,6,0,0"
                    Style="{StaticResource ListTitleStyle}"
                    Text="{x:Bind RegnumId}" />
                <TextBlock
                    Grid.Row="1"
                    Grid.Column="1"
                    Margin="12,0,0,6"
                    Style="{StaticResource ListTitleStyle}"
                    Text="{x:Bind RegnumName}" />
                <TextBlock
                    Grid.Row="1"
                    Grid.Column="1"
                    Margin="112,0,0,6"
                    Style="{StaticResource ListTitleStyle}"
                    Text="{x:Bind Subregnum}" />
                <TextBlock
                    Grid.Row="1"
                    Grid.Column="1"
                    Margin="212,0,0,6"
                    Style="{StaticResource ListTitleStyle}"
                    Text="{x:Bind Author}" />
                <TextBlock
                    Grid.Row="1"
                    Grid.Column="1"
                    Margin="312,0,0,6"
                    Style="{StaticResource ListTitleStyle}"
                    Text="{x:Bind AuthorYear}" />
            </Grid>
