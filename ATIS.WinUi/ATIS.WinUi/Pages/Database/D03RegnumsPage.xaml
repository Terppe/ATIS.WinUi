                    </ScrollViewer>
                </TabViewItem>
                <TabViewItem Header="Phylum">
                    <ScrollViewer>
                        <ContentControl
                            Content="{Binding SelectedPhylum}"
                            ContentTemplate="{StaticResource PhylumDetailTemplate}"
                            IsTabStop="True" />
                    </ScrollViewer>
                </TabViewItem>
                <TabViewItem Header="Division">
                    <ScrollViewer>
                        <ContentControl
                            Content="{Binding SelectedDivision}"
                            ContentTemplate="{StaticResource DivisionDetailTemplate}"
                            IsTabStop="True" />
                    </ScrollViewer>
                </TabViewItem>
                <TabViewItem Header="Reference">
                    <TabView>
                        <TabView.TabItems>
                            <TabViewItem Header="RefExpert">
                                <ScrollViewer>
                                    <ContentControl
