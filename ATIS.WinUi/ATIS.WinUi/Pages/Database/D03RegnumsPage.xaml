                                </ScrollViewer>
                            </TabViewItem>
                        </TabView.TabItems>
                    </TabView>
                </TabViewItem>
                <TabViewItem Header="Comments">
                    <ScrollViewer>
                        <ListView
                            BorderBrush="Black"
                            BorderThickness="1"
