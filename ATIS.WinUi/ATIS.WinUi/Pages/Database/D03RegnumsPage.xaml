        <StackPanel
            Grid.Row="0"
            HorizontalAlignment="Left"
            Orientation="Horizontal">
            <TextBox Height="35" PlaceholderText="Regnum">
                <TextBox.Text>
                    <Binding
                        Mode="TwoWay"
                        Path="SearchRegnumName"
                        UpdateSourceTrigger="PropertyChanged" />
                </TextBox.Text>
            </TextBox>
            <Button
                Width="60"
                Margin="2"
                Command="{x:Bind D03RegnumsViewModel.GetRegnumsByNameOrIdCommand, Mode=OneWay}"
                Content="Search"
                CornerRadius="15" />
            <Button
                Width="60"
                Margin="2"
                Command="{x:Bind D03RegnumsViewModel.AddRegnumCommand, Mode=OneWay}"
                Content="Add"
                CornerRadius="15" />
            <Button
                Width="60"
                Margin="2"
                Command="{x:Bind D03RegnumsViewModel.SaveRegnumCommand, Mode=OneWay}"
                Content="Save"
                CornerRadius="15" />
            <Button
                Width="60"
                Margin="2"
                Command="{x:Bind D03RegnumsViewModel.CopyRegnumCommand, Mode=OneWay}"
