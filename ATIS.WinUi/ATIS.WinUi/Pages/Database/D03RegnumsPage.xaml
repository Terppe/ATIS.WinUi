                                </ScrollViewer>
                            </TabViewItem>
                            <TabViewItem Header="RefSource">
                                <ScrollViewer>
                                    <ContentControl
                                        Content="{Binding SelectedRefSource}"
                                        ContentTemplate="{StaticResource RefSourceDetailTemplate}"
                                        IsTabStop="True" />
                                </ScrollViewer>
                            </TabViewItem>
                            <TabViewItem Header="RefAuthor">
                                <ScrollViewer>
                                    <ContentControl
                                        Content="{Binding SelectedRefAuthor}"
                                        ContentTemplate="{StaticResource RefAuthorDetailTemplate}"
                                        IsTabStop="True" />
                                </ScrollViewer>
                            </TabViewItem>
                        </TabView.TabItems>
                    </TabView>
                </TabViewItem>
                <TabViewItem Header="Comment">
                    <ScrollViewer>
                        <ContentControl
                            Content="{Binding SelectedComment}"
                            ContentTemplate="{StaticResource CommentDetailTemplate}"
                            IsTabStop="True" />
                    </ScrollViewer>
                </TabViewItem>
            </TabView.TabItems>
        </TabView>
        <TabView Grid.Row="2" Grid.Column="0">
            <TabView.TabItems>
                <TabViewItem Header="Phylums">
                    <ScrollViewer>
                        <ListView
                            BorderBrush="Black"
                            BorderThickness="1"
                            ItemTemplate="{StaticResource PhylumItemTemplate}"
                            ItemsSource="{Binding Source={StaticResource PhylumsList}}"
